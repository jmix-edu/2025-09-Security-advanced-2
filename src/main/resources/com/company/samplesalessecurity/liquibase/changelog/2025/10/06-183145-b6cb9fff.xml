<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
                      http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-latest.xsd"
        objectQuotingStrategy="QUOTE_ONLY_RESERVED_WORDS">
    <changeSet id="1" author="sample-sales-security">
        <createIndex indexName="IDX_ORDER_LINE_ORDER" tableName="ORDER_LINE">
            <column name="ORDER_ID"/>
        </createIndex>
    </changeSet>
    <changeSet id="2" author="sample-sales-security">
        <createIndex indexName="IDX_ORDER_LINE_PRODUCT" tableName="ORDER_LINE">
            <column name="PRODUCT_ID"/>
        </createIndex>
    </changeSet>
    <changeSet id="3" author="sample-sales-security">
        <createIndex indexName="IDX_ORDER__CUSTOMER" tableName="ORDER_">
            <column name="CUSTOMER_ID"/>
        </createIndex>
    </changeSet>
</databaseChangeLog>